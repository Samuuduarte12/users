 {
  "swagger": "2.0",
  "info": {
    "title": "API de Usuários",
    "version": "1.0.0"
  },
  "paths": {
    "/users": {
      "get": {
        "summary": "Lista todos os usuários",
        "responses": {
          "200": {
            "description": "Lista de usuários"
          }
        }
      },
      "post": {
        "summary": "Cria um novo usuário",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "name": { "type": "string" },
                "email": { "type": "string" },
                "password": { "type": "string" }
              }
            }
          }
        ],
        "responses": {
          "201": { "description": "Usuário criado com sucesso" }
        }
      }
    }
  }
}